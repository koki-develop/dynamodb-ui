{
  "name": "dynamodb-ui",
  "version": "0.0.1",
  "author": "Koki Sato <kou.pg.0131@gmail.com>",
  "homepage": "https://github.com/koki-develop/dynamodb-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koki-develop/dynamodb-ui.git"
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "dynamodb-ui": "dist/server/server.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite-node ./src/server/server.ts",
    "test": "vitest",
    "seed": "vite-node ./seed/seed.ts",
    "prebuild": "tsc && rm -rf ./dist",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/server/server.ts",
    "prepublishOnly": "npm run build",
    "postbuild": "node ./postbuild.cjs",
    "lint": "biome check",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{cjs,js,ts,tsx,json}": [
      "biome check --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.525.0",
    "@aws-sdk/util-dynamodb": "3.525.0",
    "@mantine/core": "7.6.1",
    "@mantine/hooks": "7.6.1",
    "@tabler/icons-react": "2.47.0",
    "@tanstack/react-query": "5.24.1",
    "express": "4.21.1",
    "pretty-bytes": "6.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.8.1",
    "react-router-dom": "6.22.2",
    "vite": "5.1.8",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/express": "4.17.21",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "@vitejs/plugin-react-swc": "3.6.0",
    "autoprefixer": "10.4.17",
    "concurrently": "8.2.2",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "postcss": "8.4.35",
    "postcss-preset-mantine": "1.13.0",
    "postcss-simple-vars": "7.0.1",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite-node": "1.3.1",
    "vitest": "1.3.1"
  }
}
